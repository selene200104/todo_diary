<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todo_diary">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".scheduleItem"></activity>
        <activity android:name=".DiaryItem" />
        <activity android:name=".loginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".TodoCheckActivity" />
        <activity android:name=".AddTodoProjectActivity" />
        <activity android:name=".TodoMainActivity" />
        <activity android:name=".AddScheduleActivity" />
        <activity android:name=".ScheduleManagementMainActivity" />
        <activity android:name=".CheckDiaryActivity" />
        <activity android:name=".WriteDiaryActivity" />
        <activity android:name=".DiaryMainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>